{
  "name": "Dark Mood",
  "version": "0.1",
  "description": "Build an Extension with Angular",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "contextMenus",
    "webNavigation"
  ]
}
